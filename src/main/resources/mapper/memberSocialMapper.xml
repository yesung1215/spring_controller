<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.oauth.mapper.MemberSocialMapper">
    <insert id="insert" parameterType="MemberSocialVO">
        INSERT INTO TBL_MEMBER_SOCIAL
        VALUES(SEQ_MEMBER_SOCIAL.NEXTVAL, #{memberSocialProviderId}, #{memberSocialProvider}, #{memberId})
    </insert>

    <select id="selectAll" parameterType="Long" resultType="String">
        SELECT MEMBER_SOCIAL_PROVIDER
        FROM TBL_MEMBER_SOCIAL
        WHERE ID = #{id}
    </select>
</mapper>